<%= simple_form_for(@company) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :contact_name %>
    <%= f.input :address_1 %>
    <%= f.input :address_2 %>
    <%= f.input :region %>
    <%= f.input :country %>
    <%= f.input :user_id %>
    <%= f.input :subscription_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
